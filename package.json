{
  "name": "fbt-top-level-repo",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "babel-plugin-fbt-runtime:test": "jest packages/babel-plugin-fbt-runtime",
    "babel-plugin-fbt:test": "jest packages/babel-plugin-fbt",
    "build": "gulp --series clean build-runtime",
    "build-runtime": "gulp build-runtime",
    "clean": "gulp clean",
    "clean-build": "pnpm install && cd ./demo-app && clean-build",
    "clean-test": "pnpm clean-build && pnpm test",
    "demo-app:test": "cd ./demo-app && pnpm test",
    "dev": "cd demo-app && pnpm dev",
    "fbt:test": "jest packages/fbt",
    "fbt:watch": "gulp watch-runtime",
    "flow:check": "flow check --show-all-errors",
    "flow:watch": "yarn watch 'yarn flow stop && yarn flow check --show-all-errors 2>&1 | tee .flow-results' . --ignoreDotFiles",
    "test": "cd ./demo-app test-build-scripts && cd .. && jest && pnpm flow:check",
    "test:watch": "jest --watch",
    "top-level:test": "pnpm jest __tests__/*"
  },
  "browserslist": "> 1%, not dead",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "babel-plugin-minify-replace": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.2.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.25.9",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@testing-library/react": "^16.0.1",
    "del": "^3.0.0",
    "flow-bin": "0.225.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-derequire": "^2.1.0",
    "gulp-each": "^0.5.0",
    "gulp-flatten": "^0.4.0",
    "gulp-header": "^2.0.9",
    "gulp-if": "^2.0.2",
    "gulp-once": "^2.1.1",
    "gulp-rename": "^1.4.0",
    "gulp-util": "^3.0.8",
    "invariant": "^2.2.4",
    "jest": "^30.0.0-alpha.6",
    "jest-environment-jsdom": "^29.7.0",
    "nullthrows": "^1.1.1",
    "prettier": "^2.0.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "react-dom": "^18.0.0",
    "webpack-stream": "^7.0.0"
  }
}
