{
  "name": "fbt-private",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r build",
    "build:all": "pnpm -r build; pnpm -r build:types",
