{
  "name": "fbtee",
  "version": "0.0.1",
  "description": "The JavaScript & React Internationalization Framework.",
  "keywords": [
    "fbt",
    "globalization",
    "i18n",
    "internationalization",
    "l10n",
    "localization",
    "react",
    "translation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/fbt"
  },
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -f lib/index.js; esbuild --target=node22 --platform=node --format=esm --outfile=lib/index.js --jsx=automatic --bundle ./src/index.tsx; cp ./src/ReactTypes.d.ts ./lib/ReactTypes.d.ts",
    "build:types": "dts-bundle-generator -o lib/index.d.ts src/index.tsx"
  },
  "dependencies": {
    "invariant": "^2.2.4"
  },
  "devDependencies": {},
  "peerDependencies": {
    "@nkzw/babel-plugin-fbtee": "workspace:*",
    "@nkzw/babel-plugin-fbtee-runtime": "workspace:*",
    "react": "^19.0.0"
  }
}
