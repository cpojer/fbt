{
  "name": "babel-plugin-fbt-runtime",
  "version": "1.0.0",
  "description": "The secondary FBT Babel transform, for E2E use",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/fbt.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -f lib/index.js; esbuild --target=node22 --platform=node --format=esm --outfile=lib/index.js --external:@babel/* --bundle ./src/index.tsx",
    "build:types": "dts-bundle-generator -o lib/index.d.ts src/index.tsx"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/traverse": "^7.26.3",
    "@babel/types": "^7.26.3",
    "invariant": "^2.2.4"
  },
  "peerDependencies": {
    "babel-plugin-fbt": "workspace:*"
  }
}
